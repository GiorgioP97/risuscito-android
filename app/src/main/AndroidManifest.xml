<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="it.cammino.risuscito"
    android:installLocation="auto"
    android:versionCode="92"
    android:versionName="3.6.0">

    <uses-sdk
        android:minSdkVersion="9"
        android:targetSdkVersion="23" />
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.READ_PHONE_STATE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.WAKE_LOCK" />
    <application
        android:name="it.cammino.risuscito.ui.RisuscitoApplication"
        android:allowBackup="true"
        android:fullBackupContent="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/RisuscitoTheme" >
        <activity
            android:name="it.cammino.risuscito.MainActivity"
            android:theme="@style/RisuscitoTheme.Launcher" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="it.cammino.risuscito.PaginaRenderActivity"
            android:label="">
        </activity>
        <activity
            android:name="it.cammino.risuscito.PaginaRenderFullScreen"
            android:label="">
        </activity>
        <activity
            android:name="it.cammino.risuscito.GeneralInsertSearch"
            android:label="" >
        </activity>
        <activity
            android:name="it.cammino.risuscito.CreaListaActivity"
            android:label="" >
        </activity>
        <activity
            android:name="it.cammino.risuscito.filepicker.ThemedFilePickerActivity"
            android:label="@string/app_name"
            android:theme="@style/FilePickerTheme">
        </activity>
        <activity android:name="it.cammino.risuscito.slides.IntroPaginaRender" android:label=""/>
        <activity android:name="it.cammino.risuscito.slides.IntroConsegnati" android:label=""/>
        <activity android:name="it.cammino.risuscito.slides.IntroCreaLista" android:label=""/>
        <activity android:name="it.cammino.risuscito.slides.IntroMain" android:label=""/>
    </application>

</manifest>
